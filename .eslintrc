{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": "airbnb/legacy",
  "rules": {
    "semi": [2, "never"],
	"func-names": 0,
	"consistent-return" : 0,
	"no-trailing-spaces" : 1,
  "no-param-reassign": 0,
  "guard-for-in" : 0,
  "no-cond-assign":0
  }
}
